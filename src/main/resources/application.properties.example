server.address={your address}
server.port={your port}

# API 호출시, SQL 문을 콘솔에 출력한다.
spring.jpa.show-sql={true | false}

# DDL 정의시 데이터베이스의 고유 기능을 사용합니다.
# ex) 테이블 생성, 삭제 등
spring.jpa.generate-ddl={true | false}

# MySQL 을 사용할 것.
spring.jpa.database=mysql

# MySQL 설정
spring.datasource.url=jdbc:mysql://{your database host}:{your database port}/{your database name}?useSSL=false&characterEncoding=UTF-8&serverTimezone=UTC
spring.datasource.username={your database ID}
spring.datasource.password={your database PW}
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver